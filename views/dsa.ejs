    </div>

    <!-- Div - 5 -->
    <div class="c5 d-flex justify-content-center">
      <div
        class="card text-black mb-3 mx-2 my-4"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-danger fw-bold">Trie</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 6</h5>
          <p class="card-text"></p>
          <a
            class="btn btn-outline-success btn-lg"
            href="trie"
            role="button"
            >Start</a
          >
        </div>
      </div>

      <div
        class="card text-black mb-3 mx-2 my-4"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-success fw-bold">Dynamic Programming</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 60</h5>
          <p class="card-text"></p>
          <a class="btn btn-outline-success btn-lg" href="dp" role="button"
