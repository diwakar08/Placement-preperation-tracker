      <div
        class="card text-black mb-3 mx-2 my-3"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-primary fw-bold">Greedy</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 35</h5>
          <p class="card-text"></p>
          <a
            class="btn btn-outline-success btn-lg"
            href="greedy"
            role="button"
            >Start</a
          >
        </div>
      </div>

      <div
        class="card text-black mb-3 mx-2 my-3"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-success fw-bold">Binary Tree</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 35</h5>
          <p class="card-text"></p>
          <a class="btn btn-outline-success btn-lg" href="bt" role="button"
            >Start</a
          >
        </div>
      </div>

      <div
        class="card text-black mb-3 mx-2 my-3"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-danger fw-bold">Binary search Tree</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 22</h5>
          <p class="card-text"></p>
          <a
            class="btn btn-outline-success btn-lg"
            href="bst"
            role="button"
            >Start</a
          >
        </div>
      </div>
    </div>

    <!-- Div 3 -->
    <div class="c3 d-flex justify-content-center">
      <div
        class="card bg-light mb-3 mx-2 my-3"
        style="width: 48vh; height: 36vh"
      >
        <div class="card-header text-danger fw-bold">Graph</div>
        <div class="card-body dflex">
          <h5 class="card-title">Total Question - 44</h5>
          <p class="card-text"></p>
          <a
            class="btn btn-outline-success btn-lg"
            href="graph"
            role="button"
